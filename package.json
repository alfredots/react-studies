{
  "name": "react-studies",
  "version": "0.1.0",
  "author": "Alfredo Tito",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings=0",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && chmod +x ./.husky/pre-commit && husky install",
    "postinstall": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:watch": "npm run test --watch",
    "generate": "npm run plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "code:check": "eslint ./src --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "code:format": "eslint ./src --ext .ts,.tsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": ["npm run code:check"],
    "src/**/*.{js,ts,tsx}": ["npm run code:check"]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@supabase/supabase-js": "^2.26.0",
    "axios": "^1.4.0",
    "eslint-config-next": "^13.4.6",
    "lucide-react": "^0.248.0",
    "next": "^13.4.7",
    "next-pwa": "^5.5.5",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-icons": "^4.10.1",
    "styled-components": "5.3.11",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@storybook/addon-essentials": "7.0.22",
    "@storybook/builder-webpack5": "^7.0.22",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "7.0.23",
    "@swc/core": "^1.3.66",
    "@swc/jest": "^0.2.26",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.15.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.2.2",
    "plop": "^3.1.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "storybook-addon-next": "^1.8.0",
    "storybook-addon-next-router": "^4.0.2",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.9.5",
    "webpack": "5.88.0"
  }
}
